{
  "name": "rollup-ts-quick-start",
  "version": "1.1.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production node_modules/.bin/rollup -c ./build/rollup.config.prod.js",
    "dev": "NODE_ENV=development node_modules/.bin/rollup -w -c ./build/rollup.config.dev.js",
    "commitizen": "commitizen init cz-conventional-changelog --save --save-exact",
    "setup": "yarn",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -p",
    "version": "standard-version -- --release-as"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "cz-conventional-changelog": "^3.0.2",
    "rollup": "^1.26.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.3",
    "standard-version": "^8.0.1",
    "tslib": "^1.10.0",
    "typescript": "^3.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
